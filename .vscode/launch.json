	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/a.test",
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		},
		{
			"name": "(lldb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.test",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb"
		},
		{
			"name": "Debug Build",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.test",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe"
            },
			"externalConsole": true,
			"logging": {
				"trace": true,
				"traceResponse": true,
				"engineLogging": true
			 },
			"preLaunchTask": "GDB C/C++ Makefile Build"
		},
		{
			"name": "Test Build",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.test",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe"
            },
			"externalConsole": true,
			"logging": {
				"trace": true,
				"traceResponse": true,
				"engineLogging": true
			 },
			"preLaunchTask": "GDB C/C++ Makefile Test Final"
		}
	]
}